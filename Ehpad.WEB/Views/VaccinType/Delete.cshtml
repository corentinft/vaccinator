@model Ehpad.ORM.VaccinType

@{
    ViewData["Title"] = "Suppression";
}

<h1>Supression</h1>

<h3>Etes-vous certain de vouloir supprimer ce type de vaccin ?</h3>
<div>
    <h4>Type de vaccin</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Label)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Label)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Supprimer" class="btn btn-danger" /> |
        <a asp-action="Index">Retour</a>
    </form>
</div>
